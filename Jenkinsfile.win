pipeline {
    agent {
		label 'jenkinsagent'
	}
    stages {
        stage('Checkout All') {
			when {
				expression {params.isPackagesToDeploy == 'all'}
            }
            steps {
				bat 'echo "In here"'
				git.exe config core.sparsecheckout true
                git.exe sparse-checkout init --cone
				git.exe sparse-checkout add is/packages
			}
        }
    }
}