pipeline {
    agent {
		label 'jenkinsagent'
	}
    stages {
        stage('Checkout All') {
			when {
				expression {params.isPackagesToDeploy == 'all'}
            }
            steps {
				- echo "In here"
                - git sparse-checkout init --cone
				- git sparse-checkout add is/packages/ is/tests/
			}
        }
    }
}